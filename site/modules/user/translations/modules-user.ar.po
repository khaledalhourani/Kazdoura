"<em>and</em> where <strong>%property</strong> is <strong>%value</strong>"
msgstr ""
"<em>و</em>في حال كون <strong>%property</strong> <strong>%value</strong>"

#: modules/user/user.admin.inc:55
msgid "<strong>%property</strong> is <strong>%value</strong>"
msgstr "<strong>%property</strong> <strong>%value</strong>"

#: modules/user/user.admin.inc:137;910 modules/user/user.module:623
msgid "Member for"
msgstr "عضو منذ"

#: modules/user/user.admin.inc:138;911
msgid "Last access"
msgstr "آخر وصول"

